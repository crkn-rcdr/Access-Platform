{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "/util/update.json",
  "title": "Access Platform update specification",
  "description": "An object that describes a request for and the output of an automated process that is applied to the parent object",
  "type": "object",
  "properties": {
    "requestDate": {
      "type": "string",
      "format": "date-time"
    },
    "processDate": {
      "type": "string",
      "format": "date-time"
    },
    "succeeded": { "type": "boolean" },
    "message": { "type": "string" }
  },
  "required": ["requestDate"]
}
